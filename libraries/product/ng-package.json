{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/libraries/@jamesbenrobb/product",
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "src/styles",
      "../styles"
    ]
  },
  "assets": [
    {
      "input": "src/assets",
      "glob": "**/*.*",
      "output": "/assets"
    },
    {
      "input": "src/styles",
      "glob": "**/*.scss",
      "output": "/styles"
    }
  ]
}
